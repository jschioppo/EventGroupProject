@{
    ViewData["Title"] = "Home Page";
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

<h2>Your Upcoming Events</h2>

<div id="upcoming-events">
    @foreach(var upcomingEvent in Model)
    {
    <div class="event">
        <div class="event-name">@upcomingEvent.EventName</div>
        <div>@upcomingEvent.City</div>
        <div>@upcomingEvent.Location</div>
        <div>@upcomingEvent.StartTime</div>
        <div>@upcomingEvent.Duration</div>
        <div>@upcomingEvent.Price</div>
    </div>
        
    }
</div>

<script type="text/javascript">
    
</script>

<style type="text/css">
    .event {
        width: 75%;
        margin: auto;
        color: #DEB887;
        margin-bottom: 15px;
        text-align: center;
    }

    .event-name{
        font-size: 24px;
        font-weight: bold;
    }

</style>


